<script>
    $(document).ready(function () {
        let table = $('#myTable').DataTable({
            select: true,
            pageLength: 5,
            stateSave: true,
            lengthMenu: [5, 10, 15, 20],
            columnDefs: [
                {
                    searchPanes: {
                        show: true
                    },
                    targets: [1, 2, 3]
                }
            ],
            "order": [[3, "desc"]],
            "language": {
                "sEmptyTable": "Không có dữ liệu trong bảng",
                "sInfo": "Đang hiển thị từ _START_ đến _END_ trong tổng số _TOTAL_ kết quả",
